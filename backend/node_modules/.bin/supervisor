#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/supervisor@0.12.0/node_modules/supervisor/lib/node_modules:/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/supervisor@0.12.0/node_modules/supervisor/node_modules:/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/supervisor@0.12.0/node_modules:/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/supervisor@0.12.0/node_modules/supervisor/lib/node_modules:/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/supervisor@0.12.0/node_modules/supervisor/node_modules:/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/supervisor@0.12.0/node_modules:/mnt/c/Users/e-ars/Documents/GitHub/DailyAnime/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../supervisor/lib/cli-wrapper.js" "$@"
else
  exec node  "$basedir/../supervisor/lib/cli-wrapper.js" "$@"
fi
